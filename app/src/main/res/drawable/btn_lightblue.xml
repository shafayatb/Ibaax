<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_focused="false" android:state_pressed="true">
        <shape android:shape="rectangle">


            <stroke android:width="0.5dip" android:color="#1E92C9" />

            <gradient android:angle="-90" android:endColor="#1E92C9" android:startColor="#1382B7" />
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp"/>
        </shape>
    </item>


    <item>
        <shape android:shape="rectangle">


            <stroke android:width="0.5dp" android:color="#1692D0" />
            <corners android:radius="2dp" />

            <gradient android:angle="-90" android:endColor="#33b5e5" android:startColor="#33b5e5" />
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp"/>
        </shape>
    </item>

</selector>